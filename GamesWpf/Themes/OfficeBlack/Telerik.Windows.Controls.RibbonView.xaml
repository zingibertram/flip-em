                                Background="{StaticResource RadRibbonBarButtonPressedBackground}"
                                BorderBrush="{StaticResource RadRibbonBarButtonPressedBorder}">
                            <Border BorderBrush="{StaticResource RadRibbonBarButtonPressedBorderInner}" CornerRadius="{TemplateBinding CornerRadius}" BorderThickness="{TemplateBinding BorderThickness}"/>
                        </Border>
                        <Border x:Name="MouseOverVisual"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="{TemplateBinding CornerRadius}"
                                Opacity="0"
                                Background="{StaticResource RadRibbonBarButtonOverBackground}"
                                BorderBrush="{StaticResource RadRibbonBarButtonOverBorder}">
                            <Border BorderBrush="{StaticResource RadRibbonBarButtonOverBorderInner}" CornerRadius="{TemplateBinding CornerRadius}" BorderThickness="{TemplateBinding BorderThickness}"/>
                        </Border>
                        <ContentPresenter x:Name="Content"
                                ContentTemplate="{TemplateBinding ContentTemplate}"
