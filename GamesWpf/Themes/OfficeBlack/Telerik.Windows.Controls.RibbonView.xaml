        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Margin" Value="0 6"/>
        <Setter Property="Foreground" Value="{StaticResource RibbonTitlebarApplicationNameBrush}"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
