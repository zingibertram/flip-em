                            <Path Margin="1 1 0 0" Data="M6.0000005,2 L7.0000005,2 L7.0000005,6 L7,7 L3.0000005,7 L3.0000005,6 L6.0000005,6 z M3.000001,2 L4.000001,2 L4.000001,3 L3.000001,3 z M0,0 L1,0 L5,0 L5,1 L1,1 L1,5 L0,5 L0,1 z" Fill="{StaticResource DialogLauncher_Fill2}"/>
