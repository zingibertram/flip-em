            <Grid x:Name="BelowBackgroundGrid" Grid.ColumnSpan="3" Visibility="Collapsed">
                <Border Background="{TemplateBinding Background}" BorderThickness="1" CornerRadius="2 2 0 0" Margin="1 1 0 0"/>
                <Border BorderThickness="1" CornerRadius="2 2 0 0" BorderBrush="{TemplateBinding BorderBrush}" Margin="0 0 1 1"/>
            </Grid>
            <Grid x:Name="AboveBackgroundGridWrapper">
                <Grid x:Name="AboveBackgroundGrid"/>
            </Grid>
            <telerikToolBar:RadToolBarPanel x:Name="PART_StripPanel" Margin="3 0 0 0" Orientation="Horizontal"/>
            <telerikRibbonView:RadRibbonDropDownButton
                    MinWidth="0"
                    telerik:StyleManager.Theme="{StaticResource Theme}"
                    Visibility="{TemplateBinding CustomizationMenuVisibility}"
                    IsOpen="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsOverflowOpen, Mode=TwoWay}"
                    DropDownIndicatorVisibility="Collapsed"
                    Width="15"
                    Height="22"
                    HorizontalAlignment="Left"
                    Grid.Column="1"
                    VerticalAlignment="Top"
                    VerticalContentAlignment="Center"
                    CornerRadius="0"
                    Padding="0">
                <ToolTipService.ToolTip>
                    <TextBlock telerik:LocalizationManager.ResourceKey="RibbonViewQATCustomize"/>
                </ToolTipService.ToolTip>
                <telerikRibbonView:RadRibbonDropDownButton.DropDownContent>
                    <StackPanel>
                        <telerikRibbonView:RadGroupHeader telerik:StyleManager.Theme="{StaticResource Theme}" telerik:LocalizationManager.ResourceKey="RibbonViewQATCustomize"/>
                        <telerikNavigation:RadMenuItem Template="{StaticResource OverflowMenuItemTemplate}" Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=HasOverflowItems, Converter={StaticResource RibbonBooleanToVisibilityConverter}}">
                            <telerikNavigation:RadMenuItem.Header>
                                <telerikToolBar:RadToolBarPanel x:Name="PART_OverflowPanel" Orientation="Vertical" VerticalAlignment="Top"/>
                            </telerikNavigation:RadMenuItem.Header>
                        </telerikNavigation:RadMenuItem>
                        <telerikNavigation:RadMenuItem
                                IsSeparator="True"
                                Padding="0"
                                Margin="0"
                                MinHeight="0"
                                Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=HasOverflowItems, Converter={StaticResource RibbonBooleanToVisibilityConverter}}"/>
                        <telerikNavigation:RadMenuItem x:Name="TogglePositionMenuItem" telerik:LocalizationManager.ResourceKey="RibbonViewQATShowBelow" Command="{x:Static telerikRibbonView:RibbonCommands.ToggleQATPositionCommand}"/>
                        <telerikNavigation:RadMenuItem IsSeparator="True" Padding="0" Margin="0" MinHeight="0"/>
                        <telerikNavigation:RadMenuItem x:Name="MinimizeMenuItem" telerik:LocalizationManager.ResourceKey="RibbonViewQATMinimize" Command="{x:Static telerikRibbonView:RibbonCommands.MinimizeRibbonCommand}"/>
                    </StackPanel>
                </telerikRibbonView:RadRibbonDropDownButton.DropDownContent>
                <Path x:Name="MainPopupButtonPath"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Margin="2 3 0 0"
                        UseLayoutRounding="True"
                        Fill="{StaticResource QatButtonArrow}"
                        Data="M 0,-2.5 L 0,-1.5 L 7,-1.5 L 7,-2.5 M 0,0 L 3.5,3.5 L 7,0"
