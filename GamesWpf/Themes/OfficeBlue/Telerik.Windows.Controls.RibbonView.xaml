        <Setter Property="TitleBarBackground" Value="{StaticResource RibbonTitlebarBackgroundBrush}"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Top"/>
        <Setter Property="ApplicationButtonStyle" Value="{StaticResource ApplicationButtonStyleOffice2010}"/>
        <Setter Property="HelpButtonStyle" Value="{StaticResource HelpButtonStyle}"/>
        <Setter Property="MinimizeButtonStyle" Value="{StaticResource MinimizeButtonStyle}"/>
        <Setter Property="ContextualGroupsStyle" Value="{StaticResource ContextualGroupsStyle}"/>
        <Setter Property="ContextualTabsStyle" Value="{StaticResource ContextualTabsStyle}"/>
        <Setter Property="WindowTitleStyle" Value="{StaticResource WindowTitleStyle}"/>
        <Setter Property="HelpButtonImageSource">
