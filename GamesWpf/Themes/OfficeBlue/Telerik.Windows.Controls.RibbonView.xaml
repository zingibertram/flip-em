    <Style x:Key="{telerik:ThemeResourceKey ThemeType={x:Type external:Office_BlueThemeExternal}, ElementType={x:Type telerikRibbonView:RadButtonGroupItem}}" TargetType="telerikRibbonView:RadButtonGroupItem" BasedOn="{StaticResource RadButtonGroupItemStyle}"/>
