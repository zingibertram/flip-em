    <Style TargetType="telerikRibbonViewPrimitives:WindowTitle" BasedOn="{StaticResource WindowTitleStyle}" x:Key="{telerik:ThemeResourceKey ThemeType={x:Type external:Office_BlueThemeExternal}, ElementType={x:Type telerikRibbonViewPrimitives:WindowTitle}}"/>
    <Style TargetType="telerikRibbonView:RadRibbonView" BasedOn="{StaticResource RadRibbonViewStyle}" x:Key="{telerik:ThemeResourceKey ThemeType={x:Type external:Office_BlueThemeExternal}, ElementType={x:Type telerikRibbonView:RadRibbonView}}"/>
