    <Style x:Key="WindowTitleStyle" TargetType="telerikRibbonViewPrimitives:WindowTitle">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Margin" Value="0 6"/>
        <Setter Property="Background" Value="Orange"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="IsHitTestVisible" Value="false"/>
        <Setter Property="IsActive" Value="True"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonViewPrimitives:WindowTitle">
                    <StackPanel Orientation="Horizontal" HorizontalAlignment="{TemplateBinding HorizontalAlignment}">
                        <TextBlock x:Name="Title" Text="{TemplateBinding Title}" Foreground="{StaticResource RibbonTitlebarTitleBrush}" TextTrimming="WordEllipsis"/>
                        <TextBlock x:Name="Divider"
                                Text="-"
                                Visibility="{TemplateBinding DividerVisibility}"
                                telerik:LocalizationManager.ResourceKey="RibbonViewWindowTitleDivider"
