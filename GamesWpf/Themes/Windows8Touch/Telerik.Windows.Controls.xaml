    <Style x:Key="{telerik1:ThemeResourceKey ThemeType={x:Type external:Windows8TouchThemeExternal}, ElementType={x:Type touch1:TouchSelector}}" TargetType="touch1:TouchSelector" BasedOn="{StaticResource TouchSelectorStyle}"/>
    <Style TargetType="telerik1:RadFluidContentControl" x:Key="RadFluidContentControlStyle">
        <Setter Property="Transition">
            <Setter.Value>
                <telerikTransitions1:FadeTransition/>
