                        <Grid>
                            <Path
                                    Data="M0,0 L36,0 L36,36 z"
                                    Fill="{telerik1:Windows8TouchResource ResourceKey=AccentBrush}"
                                    VerticalAlignment="Top"
                                    HorizontalAlignment="Right"
                                    Height="36"
                                    Width="36"
                                    SnapsToDevicePixels="True"/>
