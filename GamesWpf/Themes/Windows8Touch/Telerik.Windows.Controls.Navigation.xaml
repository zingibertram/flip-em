                                    <TranslateTransform/>
                                </TransformGroup>
                            </Path.RenderTransform>
                        </Path>
                        <Path x:Name="pathExpand"
                                Fill="{telerik1:Windows8TouchResource ResourceKey=AccentBrush}"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                RenderTransformOrigin="0.5,0.5"
                                Stretch="None"
                                Width="10"
                                Height="8"
                                Data="M 0 0 L 5 8 L 10 0 Z"
                                Opacity="0"
                                SnapsToDevicePixels="True">
