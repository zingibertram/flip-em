                                SnapsToDevicePixels="True"/>
                        <Ellipse x:Name="FocusedVisual"
                                Fill="{telerik1:Windows8TouchResource ResourceKey=InvertedBrush}"
                                Stroke="{TemplateBinding BorderBrush}"
                                StrokeThickness="1"
                                Opacity="0.05"
                                Visibility="Collapsed"
