    <Style x:Key="TreeListViewDragCueStyle" TargetType="controls:TreeListViewDragCue">
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="Foreground" Value="{telerik:Office2013Resource ResourceKey=InvertedBrush}"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Padding" Value="4,2"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="controls:TreeListViewDragCue">
                    <StackPanel>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="DropStates">
                                <VisualState x:Name="DropPossible"/>
                                <VisualState x:Name="DropImpossible">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DropImpossibleIconElement" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DropPossibleIconElement" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid HorizontalAlignment="Left" Visibility="{TemplateBinding DragPreviewVisibility}" MinHeight="17">
                            <Border
                                    BorderBrush="{telerik:Office2013Resource ResourceKey=AccentMainBrush}"
                                    CornerRadius="1"
                                    BorderThickness="1"
                                    Margin="0,0,0,-3"
                                    Background="{telerik:Office2013Resource ResourceKey=EffectAccentHighBrush}">
                                <ItemsPresenter Margin="{TemplateBinding Padding}"/>
                            </Border>
                            <Grid x:Name="DraggedItemsCountContainer" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center">
                                <Border
                                        BorderBrush="{telerik:Office2013Resource ResourceKey=HighDarkBrush}"
                                        CornerRadius="1"
                                        Background="{telerik:Office2013Resource ResourceKey=MainBrush}"
                                        MinHeight="24"
                                        BorderThickness="1"
                                        Margin="0,2,0,0"
                                        Padding="4,1">
                                    <Border.Effect>
                                        <DropShadowEffect Opacity="0.2" BlurRadius="3" Direction="315" ShadowDepth="3"/>
                                    </Border.Effect>
                                </Border>
                                <TextBlock
                                        Margin="4,1,4,2"
                                        x:Name="DraggedItemsCountText"
                                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                        FontSize="15"
                                        FontFamily="Calibri"
                                        Text="5"/>
                            </Grid>
                        </Grid>
                        <Border
                                BorderBrush="{telerik:Office2013Resource ResourceKey=HighDarkBrush}"
                                Background="{telerik:Office2013Resource ResourceKey=MainBrush}"
                                MinHeight="19"
                                BorderThickness="1"
                                CornerRadius="1"
                                Margin="0,2,0,0"
                                Padding="4,0,0,0"
                                Visibility="{TemplateBinding DragTooltipVisibility}">
                            <StackPanel Orientation="Horizontal">
                                <ContentPresenter x:Name="DropImpossibleIconElement"
                                        Content="{TemplateBinding DropImpossibleIcon}"
                                        ContentTemplate="{TemplateBinding DropImpossibleIconTemplate}"
                                        Visibility="Collapsed"/>
                                <ContentPresenter x:Name="DropPossibleIconElement" Content="{TemplateBinding DropPossibleIcon}" ContentTemplate="{TemplateBinding DropPossibleIconTemplate}"/>
                                <ContentControl
                                        Margin="2,1"
                                        Content="{TemplateBinding DragActionContent}"
                                        ContentTemplate="{TemplateBinding DragActionContentTemplate}"
                                        FontSize="15"
                                        FontFamily="Calibri"
                                        VerticalAlignment="Center"
                                        Foreground="{telerik:Office2013Resource ResourceKey=InvertedBrush}"/>
                                <ContentControl
                                        Foreground="{telerik:Office2013Resource ResourceKey=InvertedBrush}"
                                        Content="{TemplateBinding DragTooltipContent}"
                                        ContentTemplate="{TemplateBinding DragTooltipContentTemplate}"
                                        FontSize="15"
                                        Margin="2,1"
                                        FontFamily="Calibri"
                                        VerticalAlignment="Center"/>
                            </StackPanel>
                        </Border>
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="DropImpossibleIcon" Value="The icon is assigned as DataTemplate because assigning as Content triggers a bug in Silverlight"/>
        <Setter Property="DropImpossibleIconTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Grid>
                        <Path
                                Data="M3,0 L5,0 L5,3 L8,3 L8,5 L5,5 L5,8 L3,8 L3,5 L0,5 L0,3 L3,3 z"
                                Margin="1,0,0,-1"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Fill="{telerik:Office2013Resource ResourceKey=ValidationBrush}"
                                Stretch="Fill"
                                SnapsToDevicePixels="True"/>
                    </Grid>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="DropPossibleIcon" Value="The icon is assigned as DataTemplate because assigning as Content triggers a bug in Silverlight"/>
        <Setter Property="DropPossibleIconTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Path
                            Data="M3,0 L5,0 L5,3 L8,3 L8,5 L5,5 L5,8 L3,8 L3,5 L0,5 L0,3 L3,3 z"
                            Margin="1,0,0,-1"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Fill="{telerik:Office2013Resource ResourceKey=AccentMainBrush}"
                            Stretch="Fill"
                            SnapsToDevicePixels="True"/>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
    </Style>
    <Style TargetType="controls:TreeListViewDragCue" BasedOn="{StaticResource TreeListViewDragCueStyle}"/>
    <ControlTemplate x:Key="RadTreeListViewTemplate" TargetType="controls:RadTreeListView">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="GridViewActivity">
