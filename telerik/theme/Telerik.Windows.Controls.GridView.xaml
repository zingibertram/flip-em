                            <RotateTransform/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </ContentControl.RenderTransform>
                </ContentControl>
                <Path x:Name="PART_SortIndicator"
                        Data="M0,0L7,0 3.5,4z"
