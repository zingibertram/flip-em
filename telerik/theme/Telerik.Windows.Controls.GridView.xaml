                    VerticalAlignment="Stretch"
                    Grid.RowSpan="4"
                    Width="5"
                    Background="{telerik:Office2013Resource ResourceKey=EffectAccentHighBrush}"/>
            <grid:GridViewLoadingIndicator x:Name="PART_GridViewLoadingIndicator"
                    Visibility="Collapsed"
                    Grid.RowSpan="4"
                    Grid.ColumnSpan="3"
                    IsBusy="{Binding IsBusy, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"/>
            <Grid x:Name="DragBetweenItemsFeedback"
                    Grid.Row="2"
                    Height="6"
                    Visibility="Collapsed"
                    HorizontalAlignment="Left"
                    IsHitTestVisible="False"
                    VerticalAlignment="Top"
                    Margin="{TemplateBinding Padding}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="6"/>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <Ellipse Stroke="{telerik:Office2013Resource ResourceKey=AccentMainBrush}" StrokeThickness="2" HorizontalAlignment="Left" VerticalAlignment="Center" Width="6" Height="6"/>
                <Rectangle Fill="{telerik:Office2013Resource ResourceKey=AccentMainBrush}" RadiusX="2" RadiusY="2" Margin="-2,0,0,0" VerticalAlignment="Center" Height="2" Grid.Column="1"/>
            </Grid>
            <Rectangle x:Name="Background_Disabled" Fill="{telerik:Office2013Resource ResourceKey=MainBrush}" Opacity="0.5" IsHitTestVisible="False" Grid.RowSpan="5" Visibility="Collapsed"/>
        </Grid>
    </ControlTemplate>
