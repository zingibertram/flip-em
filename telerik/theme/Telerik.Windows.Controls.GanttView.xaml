    <Style TargetType="gantt:EventDecoratorContainer" BasedOn="{StaticResource EventDecoratorContainerStyle}"/>
    <Style TargetType="gantt:EventContainer" BasedOn="{StaticResource EventContainerStyle}"/>
    <Style TargetType="gantt:MilestoneContainer" BasedOn="{StaticResource MilestoneContainerStyle}"/>
    <Style TargetType="gantt:SummaryContainer" BasedOn="{StaticResource SummaryContainerStyle}"/>
    <Style TargetType="gantt:ColumnHeaderContainer" BasedOn="{StaticResource ColumnHeaderContainerStyle}"/>
    <Style TargetType="gantt:SimpleCellContainer" BasedOn="{StaticResource SimpleCellContainerStyle}"/>
    <Style TargetType="gantt:CellContainer" BasedOn="{StaticResource CellContainerStyle}"/>
    <Style TargetType="gantt:CellHighlightContainer" BasedOn="{StaticResource CellHighlightContainerStyle}"/>
    <Style TargetType="gantt:CellEditingContainer" BasedOn="{StaticResource CellEditingContainerStyle}"/>
    <Style TargetType="gantt:SimpleTreeCellContainer" BasedOn="{StaticResource SimpleTreeCellContainerStyle}"/>
    <Style TargetType="gantt:TreeCellContainer" BasedOn="{StaticResource TreeCellContainerStyle}"/>
    <Style TargetType="gantt:TreeCellHighlightContainer" BasedOn="{StaticResource TreeCellHighlightContainerStyle}"/>
    <Style TargetType="gantt:TreeCellEditingContainer" BasedOn="{StaticResource TreeCellEditingContainerStyle}"/>
    <Style TargetType="telerik:BorderContainer" BasedOn="{StaticResource BorderContainerStyle}"/>
    <Style TargetType="telerik:AlternatingBorderContainer" BasedOn="{StaticResource AlternatingBorderContainerStyle}"/>
    <Style TargetType="telerik:SpecialSlotContainer" BasedOn="{StaticResource SpecialSlotContainerStyle}"/>
    <Style TargetType="telerik:GroupTickContainer" BasedOn="{StaticResource GroupTickContainerStyle}"/>
    <Style TargetType="telerik:MajorTickContainer" BasedOn="{StaticResource MajorTickContainerStyle}"/>
    <Style TargetType="telerik:MinorTickContainer" BasedOn="{StaticResource MinorTickContainerStyle}"/>
    <Style TargetType="telerik:CurrentHourTickContainer" BasedOn="{StaticResource CurrentHourTickContainerStyle}"/>
    <Style TargetType="telerik:RowHighlightContainer" BasedOn="{StaticResource RowHighlightContainerStyle}"/>
    <Style TargetType="telerik:RowSelectionContainer" BasedOn="{StaticResource RowSelectionContainerStyle}"/>
    <Style TargetType="gantt:DragResizeSlotHighlightContainer" BasedOn="{StaticResource DragResizeSlotHighlightContainerStyle}"/>
    <Style TargetType="gantt:DragResizeMilestoneSlotHighlightContainer" BasedOn="{StaticResource DragResizeMilestoneSlotHighlightContainerStyle}"/>
    <Style TargetType="gantt:DragResizeSummarySlotHighlightContainer" BasedOn="{StaticResource DragResizeSummarySlotHighlightContainerStyle}"/>
    <Style TargetType="gantt:GanttDragResizeVisualCue" BasedOn="{StaticResource GanttDragResizeVisualCueStyle}"/>
    <Style TargetType="gantt:DropReorderHighlightContainer" BasedOn="{StaticResource DropReorderHighlightContainerStyle}"/>
    <Style TargetType="gantt:DragRelationSlotContainer" BasedOn="{StaticResource DragRelationSlotClontainerStyle}"/>
