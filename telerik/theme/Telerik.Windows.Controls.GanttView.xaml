                            Stroke="{telerik:Office2013Resource ResourceKey=StrongBrush}"
                            StrokeThickness="2"/>
                    <Path x:Name="CopyEffect"
                            Visibility="Collapsed"
                            Margin="0 3 3 3"
                            Width="10"
                            Height="10"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Data="M5,1 L5,9 M1,5 L9,5"
                            Stroke="{telerik:Office2013Resource ResourceKey=AccentMainBrush}"
                            StrokeThickness="2"/>
                    <Path x:Name="LinkEffect"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Margin="4 1 3 3"
                            RenderTransformOrigin="0.51380460803998,0.331995316624905"
                            Stretch="Fill"
                            UseLayoutRounding="False"
                            Width="6"
                            Fill="{telerik:Office2013Resource ResourceKey=AccentMainBrush}"
                            Height="13"
                            Visibility="Collapsed"
