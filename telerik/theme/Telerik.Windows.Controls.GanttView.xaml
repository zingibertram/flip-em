                    <Rectangle x:Name="Border"
                            Stroke="{TemplateBinding BorderBrush}"
                            Margin="0"
                            StrokeThickness="{TemplateBinding BorderThickness}"
                            Fill="{TemplateBinding Background}"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="AlternatingBorderContainerStyle" TargetType="telerik:AlternatingBorderContainer" BasedOn="{StaticResource BorderContainerStyle}"/>
    <Style x:Key="GroupTickContainerStyle" TargetType="telerik:GroupTickContainer">
        <Setter Property="Background" Value="{telerik:Office2013Resource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Office2013Resource ResourceKey=HighLightBrush}"/>
        <Setter Property="BorderThickness" Value="0 1 1 1"/>
        <Setter Property="FontSize" Value="13"/>
        <Setter Property="FontFamily" Value="Calibri"/>
        <Setter Property="Padding" Value="6 2 6 0"/>
        <Setter Property="MinHeight" Value="23"/>
        <Setter Property="VerticalContentAlignment" Value="Top"/>
        <Setter Property="Foreground" Value="{telerik:Office2013Resource ResourceKey=InvertedBrush}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:GroupTickContainer">
                    <Border
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Background="{TemplateBinding Background}"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch">
                        <TextBlock Text="{Binding FormattedValue}" Margin="{TemplateBinding Padding}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="MajorTickContainerStyle" TargetType="telerik:MajorTickContainer">
        <Setter Property="Background" Value="{telerik:Office2013Resource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Office2013Resource ResourceKey=HighLightBrush}"/>
        <Setter Property="Foreground" Value="{telerik:Office2013Resource ResourceKey=InvertedBrush}"/>
        <Setter Property="Padding" Value="6 2 0 0"/>
        <Setter Property="BorderThickness" Value="0 0 1 1"/>
        <Setter Property="FontSize" Value="13"/>
        <Setter Property="FontFamily" Value="Calibri"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:MajorTickContainer">
                    <Border
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Background="{TemplateBinding Background}"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch">
                        <TextBlock Text="{Binding FormattedValue, Converter={StaticResource UpperCaseStringConverter}}" Margin="{TemplateBinding Padding}"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="MinorTickContainerStyle" TargetType="telerik:MinorTickContainer">
        <Setter Property="Background" Value="{StaticResource MinorTickBackground}"/>
        <Setter Property="BorderBrush" Value="{telerik:Office2013Resource ResourceKey=HighLightBrush}"/>
        <Setter Property="BorderThickness" Value="0 0 1 1"/>
        <Setter Property="FontSize" Value="13"/>
        <Setter Property="FontFamily" Value="Calibri"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:MinorTickContainer">
                    <Border
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Background="{TemplateBinding Background}"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="CurrentHourTickContainerStyle" TargetType="telerik:CurrentHourTickContainer">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="{telerik:Office2013Resource ResourceKey=AccentMainBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Margin" Value="0 0 -1 0"/>
        <Setter Property="telerik:ZIndexManager.ZIndex" Value="15"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:CurrentHourTickContainer">
                    <Border
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Background="{TemplateBinding Background}"
                            VerticalAlignment="Stretch"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RowHighlightContainerStyle" TargetType="telerik:RowHighlightContainer">
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="BorderBrush" Value="{telerik:Office2013Resource ResourceKey=AccentMainBrush}"/>
        <Setter Property="Background" Value="{telerik:Office2013Resource ResourceKey=EffectAccentLowBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="telerik:ZIndexManager.ZIndex" Value="15"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RowHighlightContainer">
                    <Border
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Background="{TemplateBinding Background}"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Margin="{TemplateBinding Padding}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RowSelectionContainerStyle" TargetType="telerik:RowSelectionContainer">
        <Setter Property="BorderBrush" Value="{telerik:Office2013Resource ResourceKey=AccentMainBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="Background" Value="{telerik:Office2013Resource ResourceKey=EffectAccentHighBrush}"/>
        <Setter Property="telerik:ZIndexManager.ZIndex" Value="20"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RowSelectionContainer">
                    <Border
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Margin="{TemplateBinding Padding}">
                        <Border Background="{TemplateBinding Background}" Margin="1 0 1 1"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="SpecialSlotContainerStyle" TargetType="telerik:SpecialSlotContainer">
        <Setter Property="BorderBrush" Value="{telerik:Office2013Resource ResourceKey=HighLightBrush}"/>
        <Setter Property="Background" Value="{telerik:Office2013Resource ResourceKey=HighLightBrush}"/>
        <Setter Property="telerik:ZIndexManager.ZIndex" Value="10"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Margin" Value="0 0 -1 0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:SpecialSlotContainer">
                    <Grid>
                        <Rectangle x:Name="Background" Fill="{TemplateBinding Background}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Opacity="0.3"/>
                        <Rectangle x:Name="Border" Stroke="{TemplateBinding BorderBrush}" StrokeThickness="{TemplateBinding BorderThickness}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="DeadlineContainerStyle" TargetType="gantt:DeadlineContainer">
        <Setter Property="telerik:ZIndexManager.ZIndex" Value="100"/>
        <Setter Property="Margin" Value="0 0 -4 0"/>
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="gantt:DeadlineContainer">
                    <StackPanel Orientation="Horizontal">
                        <Grid Width="3" Margin="0 0 -1 0">
                            <Rectangle Width="1" Fill="{telerik:Office2013Resource ResourceKey=ValidationBrush}" Margin="0 1 0 1" Height="22"/>
                        </Grid>
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="OnTimeIndicatorContainerStyle" TargetType="gantt:OnTimeIndicatorContainer">
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="MinHeight" Value="17"/>
        <Setter Property="Padding" Value="5 0"/>
        <Setter Property="telerik:ZIndexManager.ZIndex" Value="35"/>
        <Setter Property="Margin" Value="13 -5 -30 0"/>
        <Setter Property="Foreground" Value="{StaticResource OnTimeFill}"/>
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="gantt:OnTimeIndicatorContainer">
                    <Grid>
                        <Path
                                Data="M16,8 C16,12.4 12.4,16 8,16 C3.5,16 0,12.4 0,8 C0,3.5 3.5,0 8,0 C12.4,0 16,3.5 16,8 z"
                                Visibility="{Binding OriginalEvent.IsExpired, Converter={StaticResource BooleanToVisibilityConverter}}"
                                Fill="{StaticResource DeadlineBrush}"
                                Height="16"
                                Stretch="Fill"
                                VerticalAlignment="Bottom"
                                Width="16"/>
                        <Path
                                Data="M16,8 C16,12.4 12.4,16 8,16 C3.5,16 0,12.4 0,8 C0,3.5 3.5,0 8,0 C12.4,0 16,3.5 16,8 z"
                                Visibility="{Binding OriginalEvent.IsExpired, Converter={StaticResource InvertedBooleanToVisibilityConverter}}"
                                Fill="{StaticResource OnTimeBrush}"
                                Height="16"
                                Stretch="Fill"
                                VerticalAlignment="Bottom"
                                Width="16"/>
                        <Path Stretch="Fill" Fill="{TemplateBinding Foreground}" Data="M1.5,0 L6,4.5 L6,0.0 L8,0.0 L8,8 L6,8 L0.0,8 L0.0,6 L4.5,6 L0,1.5 z" Width="8" Height="8" Margin="0 1 0 0"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
